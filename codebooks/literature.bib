@article{jollyChapelHillExpert2022,
  title = {Chapel {{Hill Expert Survey}} Trend File, 1999–2019},
  author = {Jolly, Seth and Bakker, Ryan and Hooghe, Liesbet and Marks, Gary and Polk, Jonathan and Rovny, Jan and Steenbergen, Marco and Vachudova, Milada Anna},
